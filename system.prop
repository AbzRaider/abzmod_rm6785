# Media
media.stagefright.thumbnail.prefer_hw_codecs=true
ro.vendor.mtk_flv_playback_support=1
vendor.mtk_thumbnail_optimization=true


# Faster streaming videos
media.stagefright.enable-player=true
media.stagefright.enable-meta=true
media.stagefright.enable-scan=true
media.stagefright.enable-http=true
media.stagefright.enable-rtsp=true
media.stagefright.enable-record=false



debug.mediatek.high_frame_rate_multiple_display_mode=0
debug.sf.disable_backpressure=0

persist.sys.NV_FPSLIMIT=90
persist.sys.NV_POWERMODE=1
persist.sys.NV_PROFVER=15
persist.sys.NV_STEREOCTRL=0
persist.sys.NV_STEREOSEPCHG=0
persist.sys.NV_STEREOSEP=20
persist.sys.purgeable_assets=1
ro.surface_flinger.force_hwc_copy_for_virtual_displays=true
ro.surface_flinger.has_HDR_display=true
ro.surface_flinger.has_wide_color_display=true
ro.surface_flinger.max_virtual_display_dimension=4096
ro.surface_flinger.protected_contents=true
ro.surface_flinger.use_color_management=true
ro.fb.mode=1
hw3d.force=1
persist.sys.ui.hw=1
ro.sf.compbypass.enable=0
debug.sf.hw=1

# Better Scrolling
windowsmgr.max_events_per_sec=150
ro.min_pointer_dur=8 ro.max.fling_velocity=12000
ro.min.fling_velocity=8000

# GPU Turbo Boost system properties
debug.composition.type=gpu
debug.egl.hw=1
debug.egl.profiler=1
debug.performance.tuning=1
vendor.display.disable_skip_validate=1
  vendor.perf.gestureflingboost.enable=true
  vendor.display.enable_optimize_refresh=1


#Some Default things
init.svc.power-hal-1-0=running
vendor.powerhal.boot_complete=1
vendor.powerhal.init=1
persist.log.tag.libPowerHal=S
persist.log.tag.mtkpower@impl=S
persist.log.tag.mtkpower_client=S
khungtask.enable=true
llk.enable=true
persist.sys.fuse=true

#Disable Android Logger
logcat.live=disable
debugtool.anrhistory=0 
ro.config.htc.nocheckin=1 
ro.config.nocheckin=1
rofiler.debugmonitor=false 
profiler.launch=false 
profiler.hung.dumpdobugreport=false
ro.kernel.checkjni=0
ro.kernel.android.checkjni=0
profiler.force_disable_err_rpt=1
profiler.force_disable_ulog=1

ro.surface_flinger.set_idle_timer_ms=360
ro.surface_flinger.set_touch_timer_ms=280
ro.surface_flinger.use_content_detection_for_refresh_rate=true
ro.vendor.mtk_hdr_video_support=1

#Thermal
vendor.thermal.manager.data.perf=100


# GPS
ro.oppo.mtk_gps_mpe=1
ro.vendor.mtk_gps_support=1
ro.vendor.mtk_log_hide_gps=1


# Media
ro.vendor.jpeg_decode_sw_opt=1

# Audio
